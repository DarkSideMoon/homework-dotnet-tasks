{
  "serilog": {
    "using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "writeTo": [
      {
        "name": "Console"
      }
    ]
  },
  "elasticSearchSetting": {
    "uri": "http://localhost:9200",
    "indexName": "books"
  },
  "mongoSettings": {
    "uri": "mongodb://127.0.0.1:27017",
    "databaseName": "books",
    "collectionName": "book"
  }
}
